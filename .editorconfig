root = true

[*]
charset = utf-8
end_of_line = lf
indent_size =2
indent_style = space
insert_final_newline = false
max_line_length = 120
tab_width = 2

[{*.ant,*.fxml,*.jhm,*.jnlp,*.jrxml,*.pom,*.rng,*.tld,*.wsdl,*.xml,*.xsd,*.xsl,*.xslt,*.xul}]
indent_size = 2
indent_style = tab
tab_width = 2

[{*.cjs,*.js}]
indent_size = 2
tab_width = 2
